{
  "name": "ViuF",
  "version": "1.0.0",
  "description": "The Front End application for the Vertex Script Generator - Viu",
  "repository": "ssh://git@stash.ea.com:7999/pqe/viuf.git",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "start": "rimraf dist && mkdir dist && cp src/index.html dist/index.html && webpack-dev-server --port 5000 --config webpack.config.dev.js --progress --colors",
    "build": "rimraf dist && mkdir dist && cp src/index.html dist/index.html && webpack --optimize-minimize --optimize-occurrence-order --optimize-dedupe --progress --colors",
    "build:start": "npm run build && node server",
    "lint:js": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --cache --format './node_modules/eslint-friendly-formatter'",
    "test": "cross-env NODE_ENV=test mocha ./src/**/*.spec.js --compilers js:babel-register --require chai --require sinon --reporter spec --recursive --growl"
  },
  "engines": {
    "node": ">= 4.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-friendly-formatter": "^2.0.4",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^2.4.5",
    "postcss-font-magician": "^1.4.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-css-modules": "^3.7.6",
    "react-hot-loader": "^3.0.0-beta.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "stylelint": "^6.2.2",
    "stylelint-webpack-plugin": "^0.2.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "express": "^4.13.4",
    "lodash": "^4.11.2",
    "proxy-middleware": "^0.15.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-form": "^5.2.3",
    "redux-form-schema": "0.0.3"
  }
}
