{
  "name": "react-redux-webpack-starter",
  "version": "0.0.1",
  "description": "Base application for a React + Redux application that uses Webpack and NPM scripts for the build process.",
  "repository": "https://github.com/goodbomb/react-redux-webpack-starter",
  "main": "src/main.js",
  "license": "MIT",
  "engines": {
    "node": ">= 6.11.3"
  },
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build:prod",
    "start:prod": "better-npm-run start:prod",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch",
    "lint": "eslint src"
  },
  "betterScripts": {
    "start": {
      "command": "rimraf dist && node server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "rimraf dist && mkdir dist && webpack --optimize-minimize --progress --colors --config ./webpack/webpack.config.prod.js --bail",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start:prod": {
      "command": "better-npm-run build:prod && node server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "jest --colors",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "jest --colors --watch --verbose",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:coverage": {
      "command": "jest --colors -- --coverage",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "setupFiles": [
      "./config/reactTestShim.js"
    ],
    "setupFilesAfterEnv": [
      "./config/setupTests.js"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-do-expressions": "7.5.0",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "7.5.2",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.5.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "7.5.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.5.5",
    "@babel/runtime": "7.5.5",
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "9.1.3",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-styled-components": "1.10.6",
    "better-npm-run": "0.1.1",
    "copy-webpack-plugin": "5.0.4",
    "dotenv": "8.1.0",
    "eslint": "6.2.2",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "3.0.0",
    "eslint-plugin-jest": "22.15.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "file-loader": "4.2.0",
    "jest": "24.9.0",
    "jest-styled-components": "6.3.3",
    "open": "6.4.0",
    "react-hot-loader": "4.12.11",
    "rimraf": "3.0.0",
    "style-loader": "1.0.0",
    "stylelint": "10.1.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-custom-processor-loader": "0.6.0",
    "stylelint-order": "3.0.1",
    "stylelint-processor-styled-components": "1.8.0",
    "uglify-js": "3.6.0",
